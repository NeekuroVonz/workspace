package test;

import java.util.Scanner;

public class Test {
	public static void main(String []argh){
        Scanner in = new Scanner(System.in);
        int t = in.nextInt();
        for(int i = 0; i < t; i++){
            int a = in.nextInt();
            int b = in.nextInt();
            int n = in.nextInt();
            
//            int s = a + 1 * b;
//            System.out.print(s + " ");
            for (int j = 1; j < n; j+=1) {
                int s = a + (j*2) * b;
                System.out.print(s + " ");
            }
        }
        in.close();
        
        
        
    }
}
