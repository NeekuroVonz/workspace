	public static void main1(String[] args) {
		// define Array 1D
		int[] arName = new int[5];
		int[] arName2 = new int[]{1,2,3,4,5};
		
		// use loop
		arName2[ 5 ] = 10;
		for (int i = arName2.length - 1; i >= 0; i--) {
			System.out.println(arName2);
		}
	}

}
