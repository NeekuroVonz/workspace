	private static void quiz01() {
		// TODO 1 ~ 27 +1 int -> print

		for (int i = 1; i <= 27; i++) {
			
			if (i % 5 == 0) {
				System.out.println(i);
			} else if (i == 27) {
				System.out.print(i);
			} else {
				System.out.print(i + " ");
			}
			
		}
	}
}
