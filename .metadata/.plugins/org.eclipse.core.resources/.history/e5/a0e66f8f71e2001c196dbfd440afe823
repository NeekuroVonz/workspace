	private static void quiz03() {
		Random random = new Random();
		
		int min = 1, max = 6;
		
		for (int i = 1; i <= 5; i++) {
			
			int value = random.nextInt(((max - min) + 1) + min);
			int val = value;
			
			
		}
	}
	
	
	
	private static void quiz02(int col) {
		
		int p = 2;
		
		while (p < 10) {
					
			for (int i = 1; i <= 10; i++) {
				for (int j = 0; j < col; j++) {
					if (j < 10)
					System.out.print((p+j) + " x " + i + " = " + ((p+j)*i) + "\t");
				}
				System.out.println();
			}
			
			p = p + col;
			System.out.println();
		}
	}
		
		
		
