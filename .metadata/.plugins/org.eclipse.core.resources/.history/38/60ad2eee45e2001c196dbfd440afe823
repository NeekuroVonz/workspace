package com.theplayer.kits;

// control statements
public class Controls {
	
	public static void main(String[] args) {
		boolean expression = true;
		if ( expression ) {
			System.out.println("TRUE");
			if ( expression ) {
				System.out.println("TRUE");
			} else {
				System.out.println("FALSE");
			}
		} else {
			System.out.println("FALSE");
			if ( expression ) {
				System.out.println("TRUE");
			} else {
				System.out.println("FALSE");
			}
		}
	
	}

}
