
//expression : operator , value - 1 line
//block ( statement ) - lines
//function - blocks : method
class Demo01 {

	int v = 100 + 3; // define expression for variable
	
	// start from System
	public static void main(String[] args) {
		// have to call
		todo();
	} // end
	
	void todo() {
		System.out.println(v); // call method, process for variable
	}
	
}
