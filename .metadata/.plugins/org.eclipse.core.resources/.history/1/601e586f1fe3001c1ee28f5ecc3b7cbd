package com.theplayer.kits.tutorial;

public class Robo {
	private int mynumber;
	private int babo;
	void ready(){
		mynumber = 915;
		babo = -1;
	}
	int call(){
		babo += 1;
		return babo;
	}
	int resp(int call){
		if( mynumber == call ){
			return 0;
		}else if( mynumber > call){
			// UP
			return 1;
		}else{
			// Down
			return 2;
		}
	}
	void listen(int resp) {
		
	}
}
