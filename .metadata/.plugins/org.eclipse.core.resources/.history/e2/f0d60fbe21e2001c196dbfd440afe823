 */

public class Primitive {
	public static void main(String[] args) {
		defaultTypes();
		
	}

	private static void defaultTypes() {
		// TODO Auto-generated method stub
		boolean bool; // true false : contional exp
		
		// integer
		byte	ascii; // 1 byte
		short
		int
		long
		char
		
	}
}
