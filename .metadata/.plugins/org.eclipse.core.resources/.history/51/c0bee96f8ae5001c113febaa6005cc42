package test;

import java.util.Scanner;

public class Test {
	public static void main(String []argh){
        Scanner in = new Scanner(System.in);
        int t = in.nextInt();
        for(int i = 0; i < t; i++){
            int a = in.nextInt();
            int b = in.nextInt();
            int n = in.nextInt();
            for (int j = 0; j < n; j++) {
                int s = a + 1 * b;
                for (int p = 1; p < n; p++) {
                    
                    s = s + (2 * p) * b;
                    System.out.print(s + " ");
                }
            }
        }
        in.close();
        
        
        
    }
}
