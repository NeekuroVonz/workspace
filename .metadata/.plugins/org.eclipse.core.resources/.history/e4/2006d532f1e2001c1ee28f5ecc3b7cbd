package com.theplayer.kits;

public class ArrayConsole {
	
	public static void main(String[] args) {
		int[][] a2d = new int[3][4];
		// row 3, col 4
		a2d[0] = new int[4];
		a2d[1] = new int[100];
		a2d[2] = new int[10];
		
	}
	
	
	public static void main(String[] args) {
		// define Array 1D
		int[] arName = new int[5];
		int[] arName2 = new int[]{1,2,3,4,5};
		
		// use loop
		//arName2[ 5 ] = 10;
		for (int i = arName2.length - 1; i >= 0; i--) {
			System.out.println(arName2[i]);
		}
	}

}
